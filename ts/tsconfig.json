{
    "compilerOptions": {
        "outDir":"../js",
        "module": "esnext",
        "moduleResolution":"node",
        "target":"es6",
        "jsx": "react",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "importHelpers":true,
        "suppressImplicitAnyIndexErrors": true,
        "allowSyntheticDefaultImports": true    }
}